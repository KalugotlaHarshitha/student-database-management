        <messages/>
        <name>Average</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    // Get total marks as number
    var total = parseInt(g_form.getValue('total')) || 0;

    // Calculate percentage based on 6 subjects
    var percentage = (total / 600) * 100;

    // Set the average/percentage field
    g_form.setValue('average', percentage.toFixed(2)); // rounded to 2 decimals
}
]]></script>
